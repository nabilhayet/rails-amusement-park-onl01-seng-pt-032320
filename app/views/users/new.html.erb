<%= form_for @user, url: '/signup' do |f| %>
            <%= f.label :name %>
             <%= f.text_field :name %>
            <br>
            <br>
            <%= f.label :height %>
            <%= f.number_field :height %> 
            <br>
            <br>
            <%= f.label :happiness %>
            <%= f.number_field :happiness %>
            <br>
            <br>
            <%= f.label :nausea %>
            <%= f.number_field :nausea %> 
            <br>
            <br>
            <%= f.label :tickets %>
            <%= f.number_field :tickets %> 
            <br>
            <br>
            <%= f.label :password %>
            <%= f.password_field :password %>
            <br>
            <br>
           
            <br>
            <br>
           
            <%= f.button "Create User" %>
    <% end %>